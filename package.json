{
  "[n]ame": "rtmp-rtsp-converter",
  "version": "1.0.0",
  "description": "RTMP to RTSP converter service",
  "main": "dist/app.js",
  "scripts": {
    "start": "node dist/app.js",
    "dev": "nodemon --exec ts-node src/app.ts",
    "build": "tsc",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write 'src/**/*.{ts,js}'",
    "format:check": "prettier --check 'src/**/*.{ts,js}'",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "rtmp",
    "rtsp",
    "streaming",
    "converter",
    "video"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/express": "^5.0.2",
    "@types/fluent-ffmpeg": "^2.1.27",
    "@types/node": "^22.15.21",
    "@types/node-media-server": "^2.3.7",
    "@types/uuid": "^10.0.0",
    "axios": "^1.9.0",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "fluent-ffmpeg": "^2.1.3",
    "node-media-server": "^4.0.18",
    "nodemon": "^3.1.10",
    "rtsp-streaming-server": "^2.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3",
    "uuid": "^11.1.0",
    "winston": "^3.17.0"
  },
  "devDependencies": {
    "@types/node-media-server": "^2.3.7",
    "@typescript-eslint/eslint-plugin": "^8.32.1",
    "@typescript-eslint/parser": "^8.32.1",
    "eslint": "^9.27.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.4.0",
    "prettier": "^3.5.3"
  }
}
